{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fcrcnode-desktop",
  "version": "0.1.0",
  "identifier": "org.chinanation.fcrcnode.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://127.0.0.1:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "FCRC Node Desktop",
        "width": 1360,
        "height": 920,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
